#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/james/Projects/slydes/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.1.13/node_modules/@anthropic-ai/sdk/bin/node_modules:/Users/james/Projects/slydes/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.1.13/node_modules/@anthropic-ai/sdk/node_modules:/Users/james/Projects/slydes/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.1.13/node_modules/@anthropic-ai/node_modules:/Users/james/Projects/slydes/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.1.13/node_modules:/Users/james/Projects/slydes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/james/Projects/slydes/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.1.13/node_modules/@anthropic-ai/sdk/bin/node_modules:/Users/james/Projects/slydes/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.1.13/node_modules/@anthropic-ai/sdk/node_modules:/Users/james/Projects/slydes/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.1.13/node_modules/@anthropic-ai/node_modules:/Users/james/Projects/slydes/node_modules/.pnpm/@anthropic-ai+sdk@0.71.2_zod@4.1.13/node_modules:/Users/james/Projects/slydes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@anthropic-ai/sdk/bin/cli" "$@"
else
  exec node  "$basedir/../@anthropic-ai/sdk/bin/cli" "$@"
fi
